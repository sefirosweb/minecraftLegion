{
  "name": "minecraftlegionwebclient",
  "description": "Front end client for minecraft legion server",
  "author": "Sefirosweb",
  "version": "1.2.0",
  "private": false,
  "license": "ISC",
  "homepage": "https://sefirosweb.github.io/minecraftLegionWebClient",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "bootstrap": "^5.2.3",
    "gh-pages": "^5.0.0",
    "js-cookie": "^3.0.1",
    "minecraft-assets": "^1.9.0",
    "minecraft-data": "^2.220.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.8.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "request": "^2.88.2",
    "socket.io-client": "^4.5.4",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "PUBLIC_URL=/ craco start",
    "build": "PUBLIC_URL=/ craco build && cp ./prod/default.conf ./build/ && cp ./docker-compose.yml ./build/docker-compose.yml && cp .gitignore ./build/ && cp ./README.md ./build/",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "standard": "npx standard --fix",
    "dockerbuildprod": "docker build -t minecraftlegionwebclient -f Dockerfile.prod .",
    "dockerrun": "docker run -d --rm -p:3000:3000 minecraftlegionwebclient",
    "docker": "docker run --rm -it --name minecraftLegionWebClient -v ~/.gitconfig:/etc/gitconfig -p 3000:3000 --network minecraftLegionNetwork  -v $PWD:/home/app -w /home/app -u node node:16.13-bullseye /bin/bash",
    "update": "npx npm-check-updates"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sefirosweb/minecraftLegionWebClient.git"
  },
  "bugs": {
    "url": "https://github.com/sefirosweb/minecraftLegionWebClient/issues"
  },
  "devDependencies": {
    "@craco/craco": "^7.0.0",
    "@types/js-cookie": "^3.0.2"
  }
}
